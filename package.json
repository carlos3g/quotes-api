{
  "name": "quotes-api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "https://github.com/carlos3g/quotes-api",
  "author": "carlos3g",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "build:start": "yarn build && node ./dist/index.js",
    "start": "node ./dist/index.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --inspect --ignore-watch node_modules src/index.ts",
    "typeorm:dev": "typeorm-ts-node-esm -d ./src/database/index.ts",
    "typeorm-extension:dev": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/database/index.ts",
    "db:migrations": "yarn typeorm migration:run -d ./dist/database/index.js",
    "db:seeders": "yarn typeorm-extension seed -d ./dist/database/index.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.1"
  }
}
